<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.nickpeirson.MidiMapper</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/MidiMapper</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>KeepAlive</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/tmp/MidiMapper.err</string>
	<key>StandardOutPath</key>
	<string>/tmp/MidiMapper.out</string>
</dict>
</plist>
